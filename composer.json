{
    "name": "diandi/hyperf-casbin",
    "type": "library",
    "license": "MIT",
    "version": "1.0.3",
    "keywords": [
        "hyperf",
        "casbin",
        "permission",
        "access-control",
        "authorization",
        "rbac",
        "acl",
        "abac"
    ],
    "description": "An authorization library that supports access control models like ACL, RBAC, ABAC in hyperf. ",
    "authors": [
        {
            "name": "Brady",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "hyperf/database": "^3.0",
        "hyperf/db-connection": "^3.0",
        "hyperf/cache": "^3.0",
        "hyperf/utils": "^3.0",
        "hyperf/config": "^3.0",
        "hyperf/logger": "^3.0",
        "casbin/casbin": "~3.21.0",
        "hyperf/command": "^3.0",
        "casbin/psr3-bridge": "^1.3",
        "hyperf/validation": "^3.0"
    },
    "require-dev": {
        "hyperf/testing": "^3.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Voopoo\\Casbin\\": "src/"
        }
    },
    "suggest": {
        "hyperf/command": "required hyperf/command ~2.0.0 OR required hyperf/command ~1.1.0"
    },
    "scripts": {
        "test": "co-phpunit -c phpunit.xml --colors=always"
    },
    "extra": {
        "hyperf": {
            "config": "Voopoo\\Casbin\\ConfigProvider"
        }
    }
}
